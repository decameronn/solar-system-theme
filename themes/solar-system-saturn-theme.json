{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		// Activity bar
		"activityBar.activeBorder": "#a1b0ab",
		"activityBar.background": "#1c2029",
		"activityBar.border": "#1d3041",
		"activityBar.dropBorder": "#a1b0ab",
		"activityBar.foreground": "#a1b0ab",
		"activityBar.inactiveForeground": "#494239",
		"activityBarBadge.background": "#913000",
		"activityBarBadge.foreground": "#ffffff",

		// Badge and Banner
		"badge.background": "#913000",
		"badge.foreground": "#ffffff",
		"banner.background": "#913000",
		"banner.foreground": "#ffffff",

		// Breadcrumb
		"breadcrumb.activeSelectionForeground": "#e0e0e0",
		"breadcrumb.background": "#1c2029",
		"breadcrumb.focusForeground": "#e0e0e0",
		"breadcrumb.foreground": "#cccccccc",
		"breadcrumbPicker.background": "#252526",

		// Button
		"button.background": "#913000",
		"button.foreground": "#ffffff",

		// Debugger
		"debugExceptionWidget.background": "#420b0d",
		"debugExceptionWidget.border": "#a31515",
		"debugToolBar.background": "#333333",

		// Diff
		"descriptionForeground": "#ccccccb3",
		"diffEditor.diagonalFill": "#cccccc33",
		"diffEditor.insertedTextBackground": "#9bb95533",
		"diffEditor.removedTextBackground": "#ff000033",

		// Dropdown
		"dropdown.background": "#1d3041",
		"dropdown.border": "#1d3041",
		"dropdown.foreground": "#a1b0ab",

		// Editor
		"editor.background": "#1c2029", // 1c2029
		"editor.foreground": "#a6a69d",
		"editor.findMatchHighlightBackground": "#ea5c0055",
		"editor.selectionBackground": "#264f78",
		"editorBracketMatch.background": "#0064001a",
		"editorBracketMatch.border": "#888888",
		"editorCursor.foreground": "#aeafad",
		"editorGroup.border": "#444444",
		"editorGroup.dropBackground": "#53595d80",
		"editorGroupHeader.noTabsBackground": "#1c2029",
		"editorGroupHeader.tabsBackground": "#252526",
		"editorHoverWidget.background": "#1d3041",
		"editorHoverWidget.border": "#454545",
		"editorHoverWidget.foreground": "#cccccc",
		"editorHoverWidget.statusBarBackground": "#2c2c2d",
		"editorIndentGuide.activeBackground": "#707070",
		"editorIndentGuide.background": "#404040",
		"editorLineNumber.activeForeground": "#a1b0ab",
		"editorLineNumber.foreground": "#494239",
		"editorLink.activeForeground": "#4e94ce",
		"editorMarkerNavigation.background": "#2d2d30",
		"editorMarkerNavigationError.background": "#f48771",
		"editorMarkerNavigationInfo.background": "#75beff",
		"editorMarkerNavigationWarning.background": "#cca700",
		"editorWhitespace.foreground": "#e3e4e229",
		"editorWidget.background": "#1d3041",
		"editorWidget.border": "#454545",
		"editorWidget.foreground": "#cccccc",

		// Extension Button
		"extensionButton.prominentBackground": "#49737a",
		"extensionButton.prominentHoverBackground": "#578992",

		// Icon
		"icon.foreground": "#c5c5c5",

		// Input
		"input.background": "#22415d",
		"input.foreground": "#a1b0ab",
		"input.placeholderForeground": "#a6a6a6",
		"inputOption.activeBackground": "#007fd466",
		"inputOption.activeBorder": "#007acc00",
		"inputOption.activeForeground": "#ffffff",
		"inputValidation.errorBackground": "#5a1d1d",
		"inputValidation.errorBorder": "#be1100",
		"inputValidation.infoBackground": "#063b49",
		"inputValidation.infoBorder": "#007acc",
		"inputValidation.warningBackground": "#352a05",
		"inputValidation.warningBorder": "#b89500",

		// List
		"list.activeSelectionBackground": "#094771",
		"list.activeSelectionForeground": "#ffffff",
		"list.dropBackground": "#383b3d",
		"list.highlightForeground": "#18a3ff",
		"list.hoverBackground": "#2a2d2e",
		"list.inactiveSelectionBackground": "#37373d",

		// Minimap
		"minimap.errorHighlight": "#ff1212b3",
		"minimap.findMatchHighlight": "#d18616",
		"minimap.selectionHighlight": "#264f78",
		"minimap.warningHighlight": "#cca700",

		// Panel
		"panel.background": "#1c2029",
		"panel.border": "#80808059",
		"panel.dropBorder": "#a1b0ab",
		"panelSection.border": "#80808059",
		"panelSection.dropBackground": "#53595d80",
		"panelTitle.activeBorder": "#a1b0ab",
		"panelTitle.activeForeground": "#a1b0ab",
		"panelTitle.inactiveForeground": "#494239",

		// Peek View
		"peekView.border": "#007acc",
		"peekViewEditor.background": "#001f33",
		"peekViewEditor.matchHighlightBackground": "#ff8f0099",
		"peekViewEditorGutter.background": "#001f33",
		"peekViewResult.background": "#252526",
		"peekViewResult.matchHighlightBackground": "#ea5c004d",
		"peekViewTitle.background": "#1e1e1e",

		// Ports and Progress Bar
		"ports.iconRunningProcessForeground": "#369432",
		"progressBar.background": "#a1b0ab",

		// Quick input
		"quickInput.background": "#1d3041",
		"quickInput.foreground": "#ddcfbf",
		"quickInputList.focusBackground": "#094771",
		"quickInputList.focusForeground": "#ffffff",
		"quickInputTitle.background": "#ffffff1b",

		// Scrollbar
		"scrollbarSlider.activeBackground": "#223d53",
		"scrollbarSlider.background": "#1d3041",
		"scrollbarSlider.hoverBackground": "#223d53",

		// Sidebar
		"sideBar.background": "#1c2029",
		"sideBar.border": "#1d3041",
		"sideBar.dropBackground": "#53595d80",
		"sideBar.foreground": "#ceb797",
		"sideBarSectionHeader.background": "#00000000",
		"sideBarSectionHeader.border": "#cccccc33",
		"sideBarSectionHeader.foreground": "#a1b0ab",

		// Statusbar
		"statusBar.background": "#1d3041", // 1d3041
		"statusBar.debuggingBackground": "#cc6633",
		"statusBar.debuggingForeground": "#ffffff",
		"statusBar.foreground": "#ffffff",
		"statusBar.noFolderBackground": "#1d3041",
		"statusBar.noFolderForeground": "#ffffff",
		"statusBarItem.activeBackground": "#ffffff2e",
		"statusBarItem.errorBackground": "#a1b0ab",
		"statusBarItem.errorForeground": "#ffffff",
		"statusBarItem.hoverBackground": "#ffffff1f",
		"statusBarItem.prominentBackground": "#00000080",
		"statusBarItem.prominentForeground": "#ffffff",
		"statusBarItem.prominentHoverBackground": "#0000004d",
		"statusBarItem.remoteBackground": "#16825d",
		"statusBarItem.remoteForeground": "#ffffff",

		// Tab
		"tab.activeBackground": "#1d3041",
		"tab.activeForeground": "#ffffff",
		"tab.inactiveBackground": "#1c2029",
		"tab.inactiveForeground": "#ffffff80",

		// TODO: Add Terminal color palette

		// Title Bar
		"titleBar.activeBackground": "#1d3041",
		"titleBar.activeForeground": "#cccccc",
		"titleBar.inactiveBackground": "#1d304199",
		"titleBar.inactiveForeground": "#cccccc99"
	},
	"tokenColors":[
		{
			// Comment
			"scope": "comment",
			"settings": {
				"foreground": "#a6a69d" // 494239
			}
		},
		{
			// Block and Documentation comment
			"scope": [
				"comment.block",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#a1b0ab",
			}
		},
		{
            // String
            "scope": "string",
            "settings": {
                "foreground": "#f2d790" // f2d790
            }
        },
		{
			// Numeric constant
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#a1b0ab"
			}
		},
		{
			// Built-in constants
			"scope": "constant.language",
			"settings": {
				"foreground": "#a1b0ab" // a1b0ab
			}
		},
		{
			// User-defined constants
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#c3dac3"
			}
		},
		{
			// Keywords
			"scope": "keyword",
			"settings": {
				"foreground": "#b4612d"
			}
		},
		{
			// Storage
			"scope": "storage",
			"settings": {
				"foreground": "#c3dac3"
			}
		},
		{
			// Storage type 
			"scope": "storage.type",
			"settings": {
				"foreground": "#a1b0ab"
			}
		},
		{
			// Class
			"scope": [
				"entity.name.class",
                "entity.name.type",
                "entity.name.namespace",
                "entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#a1b0ab"
			}
		},
		{
			// Inherited class
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#a1b0ab"
			}
		},
		{
			// Function
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#ee9a54" 
			}
		},
		{
			// Function parameter (not argument)
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#c27d45"
			}
		},
		{
			// Tag name
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#aa663b"
			}
		},
		{
			// Tag attribute
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ceb465"
			}
		},
		{
            // Library function
            "scope": "support.function",
            "settings": {
                "foreground": "#d5ecd4"
            }
        },
        {
            // Library constant
            "scope": "support.constant",
            "settings": {
                "foreground": "#aa663b"
            }
        },
		{
            // Library type & class
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#ee9a54"
            }
        },
		{
            // Library variable
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
		{
            // Invalid
            "scope": "invalid",
            "settings": {
                "foreground": "#c3dac3"
            }
        },
        {
            // Invalid deprecated
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#c3dac3"
            }
        },
		{
            // Diff (meta) header
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#ceb465"
            }
        },
        {
            // Markup deleted
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#3ca7ff"
            }
        },
        {
            // Markup updated
            "scope": "markup.changed",
            "settings": {
                "foreground": "#3ca7ff"
            }
        },
        {
            // Markup inserted
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#3ca7ff"
            }
        },
		{
			// Markup link
			"scope": "markup.underline.link",
			"settings": {
				"foreground": "#ceb465"
			}
		},
		{
            // Markup Quote
            "scope": "markup.quote",
            "settings": {
                "foreground": "#22aa44"
            }
        },
        {
            // Markup Styling
            "scope": [
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#aa663b"
            }
        },
        {
            // Markup Inline
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#aa663b"
            }
        },
        {
            //Markup Headings
            "scope": [
                "markup.heading",
                "markup.heading.setext"
            ],
            "settings": {
                "foreground": "#c3dac3"
            }
        }
	]
}
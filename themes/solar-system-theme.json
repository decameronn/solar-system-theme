	"tokenColors":[
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			// Comment
			"scope": [
				"comment",
				"comment.block",
				"comment.line"
			],
			"settings": {
				"foreground": "#5d6b88"
			}
		},
		{
			// Documentation comment
			"scope": "comment.block.documentation",
			"settings": {
				"foreground": "#82acce",
			}
		},
		{
			// Constant
			"scope": [
				"constant",
				"constant.character",
				"constant.character.escape",
				"constant.numeric",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.integer",
				"constant.numeric.octal",
				"constant.other",
				"constant.regexp",
				"constant.rgb-value"
			],
			"settings": {
				"foreground": "#a33600",
			}
		},
		{
			//  Entity:
			"scope": [
				"entity",
				"entity.name",
				"entity.name.class",	// TODO: what's "class"?
				"entity.name.selector"
			],
			"settings": {
				"foreground": "#ee9a54"
			}
		},
		{
			// Struct, class, enum, etc
			"scope": [
				"entity.name.type",
			],
			"settings": {
				"foreground": "#aa663b"
			}
		},		
		{
			// Function
			"scope": [
				"entity.name.function",
				"entity.name.method"
			],
			"settings": {
				"foreground": "#ca9334" // #a22c29
			}
		},
		{
			// HTML & CSS tag
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#aa663b",
			}
		},
		{
			// HTML & CSS attribute + class
			"scope":[
				"entity.other",
				"entity.other.attribute-name",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#ceb465"
			}
		},
		{
			// Latex \section
			"scope": [
				"entity.name.section"
			],
			"settings": {
				"foreground": "#ee9a54"
			}
		},
		{
			"scope": [
				// "keyword",
				// "keyword.control",
				// "keyword.operator",
				// "keyword.operator.arithmetic",
				// "keyword.operator.assignment",
				// "keyword.operator.logical",
				// "keyword.operator.new"
			],
			"settings": {
				"foreground": "#b4612d"
			}
		},
		{
			"scope": [
				"storage",
				"storage.modifier",
				"storage.type"
			],
			"settings": {
				"foreground": "#a33600",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"string",
				// "constant.other.symbol",
				// "entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#dce9b0"  // #809440 - green test
			}
		},
		{
			"scope": [
				"markup",
				"markup.changed",
				"markup.deleted",
				
				"markup.raw"
			],
			"settings": {
				"foreground": "#a33600",
			}
		},
 		{
			"scope": [
				"markup.list",
				"markup.list.numbered",
				"markup.list.unnumbered"
			],
			"settings": {
				"foreground": "#feeecd"
			}
		},
		{
			"scope": [
				"markup.underline",
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#b4612d"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#ee9a54"
			}
		},
		{
			"scope": [
				"markup.other",
				"markup.quote"
			],
			"settings": {
				"foreground": "#82acce"
			}
		},
		{
			"scope": [
				"markup.inline.raw",

{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    // Activity bar
    "activityBar.activeBorder": "#ece4ca",
    "activityBar.background": "#2b1a12",
    "activityBar.border": "#653d2a",
    "activityBar.dropBorder": "#ece4ca",
    "activityBar.foreground": "#d97f62",
    "activityBar.inactiveForeground": "#653d2a",
    "activityBarBadge.background": "#d97f62",
    "activityBarBadge.foreground": "#060606",

    // Badge and Banner
    "badge.background": "#d97f62",
    "badge.foreground": "#060606",
    "banner.background": "#d97f62",
    "banner.foreground": "#060606",

    // Breadcrumb
    "breadcrumb.activeSelectionForeground": "#e0e0e0",
    "breadcrumb.background": "#2b1a12",
    "breadcrumb.focusForeground": "#e0e0e0",
    "breadcrumb.foreground": "#cccccccc",
    "breadcrumbPicker.background": "#653d2a",

    // Button
    "button.background": "#d97f62",
    "button.foreground": "#060606",

    // Debugger
    "debugExceptionWidget.background": "#420b0d",
    "debugExceptionWidget.border": "#D4D4D3",
    "debugToolBar.background": "#333333",

    // Diff
    "descriptionForeground": "#ccccccb3",
    "diffEditor.diagonalFill": "#cccccc33",
    "diffEditor.insertedTextBackground": "#9bb95533",
    "diffEditor.removedTextBackground": "#ff000033",

    // Dropdown
    "dropdown.background": "#2b1a12",
    "dropdown.border": "#fcfced",
    "dropdown.foreground": "#ece4ca",

    // Editor
    "editor.background": "#2b1a12",
    "editor.foreground": "#e4d9b4",
    "editor.findMatchHighlightBackground": "#ea5c0055",
    "editor.selectionBackground": "#264f78",
    "editorBracketMatch.background": "#0064001a",
    "editorBracketMatch.border": "#888888",
    "editorCursor.foreground": "#aeafad",
    "editorGroup.border": "#444444",
    "editorGroup.dropBackground": "#53595d80",
    "editorGroupHeader.noTabsBackground": "#2b1a12",
    "editorGroupHeader.tabsBackground": "#2b1a12",
    "editorHoverWidget.background": "#482b1e",
    "editorHoverWidget.border": "#454545",
    "editorHoverWidget.foreground": "#ece4ca",
    "editorHoverWidget.statusBarBackground": "#2c2c2d",
    "editorIndentGuide.activeBackground": "#707070",
    "editorIndentGuide.background": "#404040",
    "editorLineNumber.activeForeground": "#d97f62",
    "editorLineNumber.foreground": "#673D2F",
    "editorLink.activeForeground": "#4e94ce",
    "editorMarkerNavigation.background": "#2d2d30",
    "editorMarkerNavigationError.background": "#d86508",
    "editorMarkerNavigationInfo.background": "#75beff",
    "editorMarkerNavigationWarning.background": "#bea11f",
    "editorWhitespace.foreground": "#e3e4e229",
    "editorWidget.background": "#653d2a",
    "editorWidget.border": "#454545",
    "editorWidget.foreground": "#cccccc",

    // Extension Button
    "extensionButton.prominentBackground": "#49737a",
    "extensionButton.prominentHoverBackground": "#578992",

    // Icon
    "icon.foreground": "#c5c5c5",

    // Input
    "input.background": "#001829",
    "input.foreground": "#ece4ca",
    "input.placeholderForeground": "#a6a6a6",
    "inputOption.activeBackground": "#007fd466",
    "inputOption.activeBorder": "#dcd7d300",
    "inputOption.activeForeground": "#ffffff",
    "inputValidation.errorBackground": "#5a1d1d",
    "inputValidation.errorBorder": "#5a1d1d",
    "inputValidation.infoBackground": "#252525",
    "inputValidation.infoBorder": "#dcd7d3",
    "inputValidation.warningBackground": "#352a05",
    "inputValidation.warningBorder": "#b89500",

    // Decorations (file color with git changes)
    "gitDecoration.modifiedResourceForeground": "#ecd8c8",

    // List
    "list.activeSelectionBackground": "#90573c",
    "list.activeSelectionForeground": "#ffffff",
    "list.errorForeground": "#dcd7d3",
    "list.warningForeground": "#dcd7d3",
    "list.dropBackground": "#383b3d",
    "list.highlightForeground": "#814e36",
    "list.hoverBackground": "#90573c",
    "list.inactiveSelectionBackground": "#814e36",

    // Minimap
    "minimap.errorHighlight": "#ff1212b3",
    "minimap.findMatchHighlight": "#d18616",
    "minimap.selectionHighlight": "#264f78",
    "minimap.warningHighlight": "#cca700",

    // Panel
    "panel.background": "#2b1a12",
    "panel.border": "#80808059",
    "panel.dropBorder": "#ece4ca",
    "panelSection.border": "#80808059",
    "panelSection.dropBackground": "#53595d80",
    // Panel - terminal
    "panelTitle.activeBorder": "#ece4ca",
    "panelTitle.activeForeground": "#ece4ca",
    "panelTitle.inactiveForeground": "#d18616",

    // Peek View
    "peekView.border": "#dcd7d3",
    "peekViewEditor.background": "#0d0d0d",
    "peekViewEditor.matchHighlightBackground": "#ff8f0099",
    "peekViewEditorGutter.background": "#0d0d0d",
    "peekViewResult.background": "#653d2a",
    "peekViewResult.matchHighlightBackground": "#ea5c004d",
    "peekViewTitle.background": "#1e1e1e",

    // Ports and Progress Bar
    "ports.iconRunningProcessForeground": "#369432",
    "progressBar.background": "#e4d1e4",

    // Quick input
    "quickInput.background": "#2b1a12",
    "quickInput.foreground": "#ece4ca",
    "quickInputList.focusBackground": "#814e36",
    "quickInputList.focusForeground": "#ffffff",
    "quickInputTitle.background": "#ffffff1b",

    // Scrollbar
    "scrollbarSlider.activeBackground": "#734530",
    "scrollbarSlider.background": "#90583c",
    "scrollbarSlider.hoverBackground": "#814e36",

    // Sidebar
    "sideBar.background": "#2b1a12",
    "sideBar.border": "#653d2a",
    "sideBar.dropBackground": "#53595d80",
    "sideBar.foreground": "#ece4ca",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#cccccc33",
    "sideBarSectionHeader.foreground": "#ece4ca",

    // Statusbar
    "statusBar.background": "#653d2a",
    "statusBar.debuggingBackground": "#cc6633",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#653d2a",
    "statusBar.noFolderForeground": "#ffffff",
    "statusBarItem.activeBackground": "#ffffff2e",
    "statusBarItem.errorBackground": "#e4d1e4",
    "statusBarItem.errorForeground": "#ffffff",
    "statusBarItem.hoverBackground": "#ffffff1f",
    "statusBarItem.prominentBackground": "#00000080",
    "statusBarItem.prominentForeground": "#ffffff",
    "statusBarItem.prominentHoverBackground": "#0000004d",
    "statusBarItem.remoteBackground": "#df5c16",
    "statusBarItem.remoteForeground": "#ffffff",

    // Tab
    "tab.activeBackground": "#653d2a",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#653d2a90",
    "tab.inactiveForeground": "#ffffff80",

    // Terminal
    "terminal.foreground": "#ece4ca",
    
    // Title Bar
    "titleBar.activeBackground": "#653d2a",
    "titleBar.activeForeground": "#cccccc",
    "titleBar.inactiveBackground": "#653d2a90",
    "titleBar.inactiveForeground": "#cccccc99",
    "titleBar.border": "#2b1a12"
  },
  "tokenColors": [
    {
      // Comment
      "scope": "comment",
      "settings": {
        "foreground": "#ffb41f"
      }
    },
    {
      // Block and Documentation comment
      "scope": [
        "comment.block",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#ffbb33"
      }
    },
    {
      // String
      "scope": "string",
      "settings": {
        "foreground": "#ffdd99"
      }
    },
    {
      // Numeric constant
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      // Built-in constants
      "scope": "constant.language",
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      // User-defined constants
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      // Keywords
      "scope": "keyword",
      "settings": {
        "foreground": "#ed8531"
      }
    },
    {
      // Storage
      "scope": "storage",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f5bd8e"
      }
    },
    {
      // Storage type 
      "scope": "storage.type",
      "settings": {
        "foreground": "#a6643d"
      }
    },
    {
      // Class
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#953e0f"
      }
    },
    {
      // Inherited class
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#df5c16"
      }
    },
    {
      // Function
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#df5c16"
      }
    },
    {
      // Function parameter (not argument)
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f5bd8e"
      }
    },
    {
      // Tag name
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#a6643d"
      }
    },
    {
      // Tag attribute
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ed8531"
      }
    },
    {
      // Library function
      "scope": "support.function",
      "settings": {
        "foreground": "#cc5514"
      }
    },
    {
      // Library constant
      "scope": "support.constant",
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      // Library type & class
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ece4ca"
      }
    },
    {
      // Library variable
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
      }
    },
    {
      // Invalid
      "scope": "invalid",
      "settings": {
        "foreground": "#df5c16"
      }
    },
    {
      // Invalid deprecated
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#cc5514"
      }
    },
    {
      // Diff (meta) header
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ece4ca"
      }
    },
    {
      // Markup deleted
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#cc5514"
      }
    },
    {
      // Markup updated
      "scope": "markup.changed",
      "settings": {
        "foreground": "#ece4ca"
      }
    },
    {
      // Markup inserted
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      // Markup link
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#cc5514"
      }
    },
    {
      // Markup Quote
      "scope": "markup.quote",
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      // Markup Styling
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#ece4ca"
      }
    },
    {
      // Markup: Emphasis
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      // Markup Inline
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#964b0d"
      }
    },
    {
      //Markup Headings
      "scope": [
        "markup.heading",
        "markup.heading.setext"
      ],
      "settings": {
        "foreground": "#df5c16"
      }
    }
  ]
}